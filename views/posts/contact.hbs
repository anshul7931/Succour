<!DOCTYPE>
<html ng-app>
<head>
    
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
    <title> contact list app </title>
</head>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular.min.js"></script>
<script src='/js/app/posts/post.js'></script>
<body >
    <div class="container" ng-controller="AppCtrl1">
        <h1> Available Contact List </h1>

        <table class="table">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Number</th>
                    <th>Occupation</th>
                </tr>
            </thead>
            <tbody>
                
                <tr ng-repeat="contact in contactlist">
                    <td><span ng-bind="contact.name"></span></td>
                    <td><span ng-bind="contact.email"></span></td>
                    <td><span ng-bind="contact.number"></span></td>
                    <td><span ng-bind="contact.occupation"></span></td>
                   
                </tr>
            </tbody>    
        </table>
    </div>


</body>
</html>